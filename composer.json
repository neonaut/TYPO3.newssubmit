{
	"name": "t3ext/newssubmit",
	"description": "Submit News and store them.",
	"type": "typo3-cms-extension",
	"version": "2.0.0",
	"require": {
		"typo3/cms-core": "^8.7"
	},
	"autoload": {
		"psr-4": {
			"T3ext\\Newssubmit\\": "Classes/"
		}
	},
	"keywords": ["news", "TYPO3", "newssubmit"],
	"homepage": "https://github.com/kaystrobach/TYPO3.newssubmit/",
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web",
			"extension-key": "newssubmit"
		}
	}
}